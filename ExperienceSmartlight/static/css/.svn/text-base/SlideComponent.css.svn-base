/*position: relative;*/

/*width:100%;*/

/*height: 100%;*/

/*}*/

.rangeDiv h2 {
  display: inline-block;
  position: relative;
  font-size: 1.2rem;
  font-family: '黑体';
  left: 10px;
  color: #848484;
  top: 0.7rem;
}

.range {
  display: inline-block;
  position: absolute;
  width: 90%;
  left: 0;
  right: 0;
  margin: auto;
  top: 2rem;
  /*width:100%;*/
  /*margin:1rem auto 0;*/
  /*color:#ffffff;*/
}

.range .label1,
.label2 {
  position: absolute;
  width: 1.33rem;
  height: 1.33rem;
  font-size: 15px;
  font-family: '黑体';
  color: black;
  text-align: right;
  vertical-align: middle;
  top: 2.6rem;
}

.range .label1 {
  left: 0;
  background-image: url("../../static/img/highImg.png");
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}

.range .label2 {
  right: 0;
  background-image: url("../../static/img/lowImg.png");
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}

.range .label1.off,
.label2.off {
  opacity: 0;
}

