input[type=range] {
  -webkit-appearance: none;
  width: 100%;
  border-radius: 10px;
  /*这个属性设置使填充进度条时的图形为圆角*/
}

input[type=range]:focus {
  outline: none;
}

/*滑轨的宽度,样式*/

.t-range input[type=range]::-webkit-slider-runnable-track {
  height: 5px;
  /*滑轨的宽度*/
  border-radius: 10px;
  /*将轨道设为圆角的*/
  /*background: -webkit-linear-gradient(left, black , white); */
  background: -webkit-linear-gradient(left,#ff0000,#ff7f00,#ffff00,#80ff00,#00ff00,#00ff80,#00ffff,#0080ff,#0000ff,#8000ff,#ff00ff,#ff007f);
  /*box-shadow: 0 1px 1px #def3f8, inset 0 .125em .125em #0d1112; !*轨道内置阴影效果*!*/
}

.t-range.do input[type=range]::-webkit-slider-runnable-track {
  background: grey;
}

/*滑块颜色*/

input[type=range]::-webkit-slider-thumb {
  -webkit-appearance: none;
  height: 25px;
  width: 25px;
  margin-top: -10px;
  /*设置滑块在滑轨上的位置(上下)*/
  background: #fdfdfd;
  border-radius: 50%;
  /*外观设置为圆形*/
  border: solid 0.125em rgba(205, 224, 230, 0.5);
  /*设置边框颜色*/
  /*box-shadow: 0 .125em .125em #3b4547; !*添加底部阴影*!*/
}

.t-range2 input[type=range]::-webkit-slider-runnable-track {
  height: 5px;
  border-radius: 10px;
  /*将轨道设为圆角的*/
  /*background: -webkit-linear-gradient(left, black , white);*/
  background: -webkit-linear-gradient(left,#575555,grey, white);
  box-shadow: 0 1px 1px #def3f8, inset 0 .125em .125em rgba(192, 192, 192, 0.89);
  /*轨道内置阴影效果*/
}

.t-range2.do input[type=range]::-webkit-slider-runnable-track {
  background: grey;
}

/*控制替代的游标  他之前的思路是隐藏本来的滑动条,自己设置一个*/

.t-range2 .__range>label>i {
  position: absolute;
  width: 1.5rem;
  height: 1.5rem;
  line-height: 2.3rem;
  /*background:rgb(255, 159, 187);*/
  background: white;
  border-radius: 0rem;
  /*border:.18rem solid rgb(255,211,224);*/
  border: .18rem solid white;
  z-index: 6666;
  text-align: center;
  color: #b4b4b4;
  font-weight: bold;
  font-size: 1rem;
  top: -0.8rem;
  left: 0;
  box-sizing: border-box;
  /*font-family:Arial;*/
  opacity: 1;
}