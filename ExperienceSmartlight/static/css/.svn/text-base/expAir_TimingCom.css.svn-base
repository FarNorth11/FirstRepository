.RepeatAdd {
  width: 94%;
  position: absolute;
  top: 0%;
  height: 48px;
  background: #4c91fc;
  padding: 35px 3% 0;
  z-index: 999;
}

.RepeatAdd div {
  font-size: 16px;
  height: 2rem;
  text-align: center;
  line-height: 2rem;
  color: white;
  font-family: 黑体;
}

.RepeatAdd span:nth-child(1) {
  width: 20px;
  height: 20px;
  background: url("../img/return.png") no-repeat;
  display: inline-block;
  background-size: cover;
  position: absolute;
  left: 7%;
}

