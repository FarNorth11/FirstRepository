@import url(../../../common/static/reset.css);
@import url(../../../common/static/common.css);
/* @import url(../../../common/static/mediaQuery.css); */
html,body,.app-body {
    height: 100%;
    overflow: hidden;
}
.app-body, .slide-up, .slide-down,main {
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    position: absolute;
}

.app_bgimghg{
    position: relative;
    width: 100%;
    height: 50%;
    opacity: 1;
    background: url('../img/bgImg_warm.jpg') no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
    text-align: center;
}
.app_bgimghg.cold{
    background: url('../img/bgImg_cold.jpg') no-repeat;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}

.lightDiv{
    position: absolute;
    top: 0%;
    left: 45%;
    height: 70%;
    width: 30%;
    margin: -15% 0 0 -10%;
    /*background: url('../img/light_1.png') no-repeat;*/
    background-image: url('../img/lightList/light_0000.png');
    opacity: 1;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
    text-align: center;
    z-index: 6;
}

.lightDiv.off{
    display: none;
}
.turnDiv_shadow{
    position: absolute;
    top: 0%;
    left: 45%;
    height: 70%;
    width: 30%;
    margin: -15% 0 0 -10%;
    /*background: url('../img/light_1.png') no-repeat;*/
    background-image: url('../img/lightList/light_0000.png');
    opacity: 1;
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
    text-align: center;
    z-index: 5;
}
.turnDiv_shadow.off{
    background-image: url('../img/lightList/light_off.png');
    -webkit-background-size: 100% 100%;
}

.turnDiv{
    position: absolute;
    display: inline-block;
    width: 3.22rem;
    height: 3.22rem;
    text-align: center;
    background: red;
    bottom: 2rem;
    right:1.5rem;
    background: url('../img/icon-turnOff.png') no-repeat 0 0;
    background-size: 100% 100%;
}

.turnDiv span{
    color: #ffffff;
    font-size: 1.2rem;
    /* padding-top: 3.03rem; */
    padding-top: 3.52rem;
    display: inline-block;
}

.enableView.off{
    position: absolute;
    height: 50%;
    width: 100%;
    background: grey;
    opacity: .5;
    top:50%;
    z-index: 999999;

}

.btnListDiv{
    position: relative;
    height: 18%;
    width: 100%;
    border-bottom: solid 1px #d6d6d6;
}
.btnListDiv .mode{
    display: inline-block;
    width: 20%;
    padding-top: 32px;
    /*padding-bottom: 27px;*/
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 40px 40px;
    color: #848484;
    font-size: 1.2rem;
    line-height: 10rem;
    text-align: center;
}

.btnListDiv .mode.on{
    color: #4c91fc;
}

.slideDiv{
    position: relative;
    height: 32%;
    width: 100%;
}

.slideDiv .colorSlide{
    width: 100%;
    height:100%;

}

.slideDiv .saturationSlide{
    width: 100%;
    height:100%;

}

